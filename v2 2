using System;

namespace ConsoleApp2
{
    class Program
    {
        static void Main(string[] args)
        {
            int A = Convert.ToInt32(Console.ReadLine());
            int B = Convert.ToInt32(Console.ReadLine());
            int sum = 0;
            for (int i=A; i<=B; i++)
            {
                sum += i * i;
            }
            Console.WriteLine(sum);
        }
    }
}
