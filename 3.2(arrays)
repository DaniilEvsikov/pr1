using System;

namespace _1._1
{
    class Program
    {
        static void Main(string[] args)
        {
            int N = Convert.ToInt32(Console.ReadLine());
            var a = Array.ConvertAll(Console.ReadLine().Split(' '), s => int.Parse(s));
            for (int i =0; i <N; i++)
            {
                if(a[i]<15)
                {
                    a[i]*=2;
                }
                Console.Write(a[i]);
                Console.Write(" ");
            }
        }
    }
}
