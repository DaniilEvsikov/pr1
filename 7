using System;

namespace ConsoleApp2
{
    class Program
    {
        static void Main(string[] args)
        {
            int cou = 1, i = 1;
            int p = Convert.ToInt32(Console.ReadLine());
            cou = p;
            do
            {
                cou *= i;
                i++;
            }
            while (i != p);
            Console.WriteLine(cou);
        }
    }
}
